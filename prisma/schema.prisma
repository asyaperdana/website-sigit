// This is your Prisma schema file

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model UserReview {
  id        Int      @id @default(autoincrement())
  userName  String
  rating    Int
  comment   String
  createdAt DateTime @default(now())
}
